public class ContadorDeNumeros
{
    public ContadorDeNumeros()
    {
       
    }
    
    public void contaAteTres() {
    
        for(int i = 1; i <= 3; i++) {
            System.out.println(i);
        }
        
        //ctrl + k -> limpa terminal
        //ctrl + t -> mostra terminal
    }
}
