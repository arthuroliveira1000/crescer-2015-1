public class Ator {

	private String nome;

	public static void main(String[] args) {

	}

	public Ator(String nome) {
		this.nome = nome;
	}
	
	public String toString() {
		return this.nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

}
