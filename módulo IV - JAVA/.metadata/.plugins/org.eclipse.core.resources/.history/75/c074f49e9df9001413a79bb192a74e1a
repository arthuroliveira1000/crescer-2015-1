import java.util.ArrayList;
import java.util.List;

public class Filme {

	private String nome;
	private List<Ator> elenco = new ArrayList<Ator>();

	public static void main(String[] args) {

	}

	public ArrayList<Ator> listaElenco(String nome) {
		for (int x = 0; x < elenco.size(); x++) {
			if (elenco[x].getNome().contains(nome)) {

			}
		}

	}

	public List<Ator> getElenco() {
		return elenco;
	}

	public void setElenco(List<Ator> elenco) {
		this.elenco = elenco;
	}

	public void adicionaAtor(Ator atorinserido) {
		elenco.add(atorinserido);
	}

	public String toString() {
		return this.nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	

}
