import java.util.ArrayList;

public class Filme {

	private String nome;
	private ArrayList<Ator> elenco = new ArrayList<Ator>();

	public static void main(String[] args) {

		
	
	}

	public static adicionaAtor(Ator atorinserido) {
		return this.elenco.add(atorinserido);
	}
	
	
	public String toString() {
		return this.nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public ArrayList<Ator> getElenco() {
		return elenco;
	}

	public void setElenco(ArrayList<Ator> elenco) {
		this.elenco = elenco;
	}

}
